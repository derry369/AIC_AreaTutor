    <!-- http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#Circle -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#Circle"/>
    


    <!-- http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#CircleAreaFormula -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#CircleAreaFormula">
        <rdf:type rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#AreaFormula"/>
        <untitled-ontology-29:usesComponent rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#CircleRadiusExample1"/>
        <untitled-ontology-29:formulaText>pi * radius * radius</untitled-ontology-29:formulaText>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#CircleExample1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#CircleExample1">
        <rdf:type rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#WorkedExample"/>
        <untitled-ontology-29:hasComponent rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#CircleRadiusExample1"/>
        <untitled-ontology-29:stepText>Identify the radius of the circle.  
Use the formula: Area = π * radius * radius.  
Substitute the radius value (e.g., 7 units).  
Calculate the area: Area = π * 7 * 7 = 153.94 units².</untitled-ontology-29:stepText>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#CircleLesson -->

