    <!-- http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramBaseExample1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramBaseExample1">
        <rdf:type rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#Base"/>
        <untitled-ontology-29:value rdf:datatype="http://www.w3.org/2001/XMLSchema#float">7.0</untitled-ontology-29:value>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramExample1 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramExample1">
        <rdf:type rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#WorkedExample"/>
        <untitled-ontology-29:hasComponent rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramBaseExample1"/>
        <untitled-ontology-29:hasComponent rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramHeightExample1"/>
        <untitled-ontology-29:stepText>Identify the base and vertical height of the parallelogram.  
Use the formula: Area = base * height.  
Substitute the values (Base = 7 units, Height = 5 units).  
Calculate the area: Area = 7 * 5 = 35 units²</untitled-ontology-29:stepText>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramHeightExample1 -->

