    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramLesson -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramLesson">
        <rdf:type rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#Lesson"/>
        <untitled-ontology-29:explainsFormula rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramAreaFormula"/>
        <untitled-ontology-29:hasWorkedExample rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#ParallelogramExample1"/>
        <untitled-ontology-29:teachesShape rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#Parallelogram"/>
        <untitled-ontology-29:lessonText>A parallelogram has opposite sides parallel and equal.
Its area depends on the base and the vertical height.</untitled-ontology-29:lessonText>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#Rectangle -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#Rectangle"/>
    


