        <untitled-ontology-29:usesComponent rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#RectangleLengthExample1"/>
        <untitled-ontology-29:usesComponent rdf:resource="http://www.semanticweb.org/dee/ontologies/2025/11/untitled-ontology-29#RectangleWidthExample1"/>
